<!--WR-INFO-->
<% @issues.each do |issue| %>
    <div style="page-break-before: always; page-break-after: always; width: 100%;">
      <%= render partial: 'attributes', locals: {issue: issue}%>
      <!--WR-INFO END-->

      <!--WR-FILES-->

      <%= render partial: 'files', locals: {issue: issue}%>
      <!--WR-FILES END-->

      <!--WR-IMAGES-->

      <%= render partial: 'images', locals: {issue: issue}%>
      <!--WR-IMAGES END-->

      <!--WR-HISTORY-->

      <%= render partial: 'history', locals: {issue: issue}%>
      <!--WR-HISTORY END-->
    </div>

<% end %>